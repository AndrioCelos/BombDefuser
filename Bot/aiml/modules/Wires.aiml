<?xml version="1.0" encoding="UTF-8"?>
<aiml>
	<category>
		<pattern>SelectingModule Wires</pattern>
		<template><srai>SelectModule Wires XS Wires</srai><reply>read</reply></template>
	</category>

	<topic name='Wires'>
		<category>
			<pattern>read ^</pattern>
			<template>
				<oob><takescreenshot token='Read Wires'/></oob>
			</template>
		</category>
		<category>
			<pattern>OOB ScreenshotReady Read Wires *</pattern>
			<template>
				<srai>XRead <sraix service='BombDefuserAimlService'>Wires <star/> 837 393 1118 393 833 679 1124 679</sraix></srai>
			</template>
		</category>

		<category>
			<pattern>XRead * * *</pattern>
			<template>
				<set name='WireCount'>3</set> wires: <star index='1'/>, <star index='2'/>, <star index='3'/>.
				<reply>cut the [nth] wire</reply>
			</template>
		</category>
		<category>
			<pattern>XRead * * * *</pattern>
			<template>
				<set name='WireCount'>4</set> wires: <star index='1'/>, <star index='2'/>, <star index='3'/>, <star index='4'/>.
				<reply>cut the [nth] wire</reply>
			</template>
		</category>
		<category>
			<pattern>XRead * * * * *</pattern>
			<template>
				<set name='WireCount'>5</set> wires: <star index='1'/>, <star index='2'/>, <star index='3'/>, <star index='4'/>, <star index='5'/>.
				<reply>cut the [nth] wire</reply>
			</template>
		</category>
		<category>
			<pattern>XRead * * * * * *</pattern>
			<template>
				<set name='WireCount'>6</set> wires: <star index='1'/>, <star index='2'/>, <star index='3'/>, <star index='4'/>, <star index='5'/>, <star index='6'/>.
				<reply>cut the [nth] wire</reply>
			</template>
		</category>

		<category>
			<pattern>Cut the * wire</pattern>
			<template><srai>cut wire <map name='ParseOrdinal'><star/></map></srai></template>
		</category>
		<category>
			<pattern>Cut the last wire</pattern>
			<template><srai>cut wire <get name='WireCount'/></srai></template>
		</category>
		<category>
			<pattern>cut wire <set>number</set></pattern>
			<template>
				<think>
					<set var='n'><star/></set>
					<set var='commands'>a</set>
					<set var='d'><srai>XSubtract <star/> XS <srai>XCoalesce <get><name>Wires <get name='module'/> Cursor</name></get> XS 1</srai></srai></set>
					<condition var='d'>
						<li value='-5'><set var='commands'>up up up up up a</set></li>
						<li value='-4'><set var='commands'>up up up up a</set></li>
						<li value='-3'><set var='commands'>up up up a</set></li>
						<li value='-2'><set var='commands'>up up a</set></li>
						<li value='-1'><set var='commands'>up a</set></li>
						<li value='1'><set var='commands'>down a</set></li>
						<li value='2'><set var='commands'>down down a</set></li>
						<li value='3'><set var='commands'>down down down a</set></li>
						<li value='4'><set var='commands'>down down down down a</set></li>
						<li value='5'><set var='commands'>down down down down down a</set></li>
						<li><set var='commands'>a</set></li>
					</condition>
					<set><name>Wires <get name='module'/> Cursor</name><star/></set>
				</think>
				<srai>ModuleSubmit <get var='commands'/></srai>
			</template>
		</category>
	</topic>
</aiml>
