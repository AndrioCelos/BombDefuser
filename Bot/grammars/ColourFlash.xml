<grammar version='1.0' xml:lang='en-gb' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root'>
		<one-of>
			<item><ruleref uri='bomb.xml'/></item>
			<item> read </item>
			<item> press <one-of><item> Yes </item><item> No </item></one-of>
				<one-of>
					<item> any time </item>
					<item>
						on
						<one-of>
							<item><ruleref uri='bomb.xml#digit'/></item>
							<item> the
								<one-of>
									<item> first </item>
									<item> second </item>
									<item> third </item>
									<item> fourth </item>
									<item> fifth </item>
									<item> sixth </item>
									<item> seventh </item>
									<item> eighth </item>
								</one-of>
								<one-of>
									<item> word </item>
									<item> element </item>
									<item> entry </item>
								</one-of>
							</item>
						</one-of>
					</item>
				</one-of>
			</item>
		</one-of>
	</rule>
</grammar>
