<grammar version='1.0' xml:lang='en-gb' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root' scope='public'>
		<one-of>
			<item><ruleref uri='#selectModule'/></item>
			<item><ruleref uri='#globalModuleCommand'/></item>
			<item> say again </item>
			<item> edgework </item>
		</one-of>
	</rule>
	<rule id='globalModuleCommand'>
		<one-of>
			<item>
				knob <item repeat='0-1'><ruleref uri='#number2'/></item> is
				<one-of>
					<item> up </item>
					<item> right </item>
					<item> down </item>
					<item> left </item>
				</one-of>
			</item>
		</one-of>
	</rule>
	<rule id='selectModule'>
		<one-of>
			<item> module <ruleref uri='#number2'/></item>
			<item> first module </item>
			<item> next module </item>
			<item>
				<item>
					select
					<item repeat='0-1'><one-of><item> a </item><item> an </item><item> the </item></one-of></item>
				</item>
				<ruleref uri='#module'/>
			</item>
			<item> vanilla modules </item>
		</one-of>
	</rule>
