<grammar version='1.0' xml:lang='en-gb' root='root' xmlns='http://www.w3.org/2001/06/grammar'>
	<rule id='root' scope='public'>
		<one-of>
			<item> veemo </item>

			<item><ruleref uri='#selectModule'/></item>
			<item><ruleref uri='#globalModuleCommand'/></item>
			<item> say again </item>
			<item> serial number </item>
			<item> edgework </item>
			<item> bomb time </item>
			<item> how much time
				<item repeat='0-1'><one-of><item> remains </item><item> is left </item><item> do we have </item></one-of></item>
			</item>
			<item>
				<one-of><item> stop </item><item> no </item><item> disable</item>
					<item> start </item><item> restore </item><item> resume </item><item> enable </item></one-of>
				module
				<one-of><item> notifications </item><item> warnings </item><item> announcements </item></one-of>
			</item>
		</one-of>
	</rule>
	<rule id='globalModuleCommand'>
		<one-of>
			<item>
				knob <ruleref uri='#number2'/> is
				<one-of>
					<item> up </item>
					<item> right </item>
					<item> down </item>
					<item> left </item>
				</one-of>
			</item>
			<item> turn the keys </item>
			<item> rotary phone <ruleref uri='#number2'/> is <item repeat='3'><ruleref uri='#digit'/></item></item>
			<!--<item><item repeat='0-1'> Souvenir answer is </item><ruleref uri='souvenir.xml#answer'/></item>-->
			<item>
				<item repeat='0-1'>
					HTTP <item repeat='0-1'> Response </item><item repeat='0-1'>
					<ruleref uri='#number2'/></item> is
				</item>
				<item repeat='3'><ruleref uri='#digit'/></item>
			</item>
			<item>
				<item repeat='0-1'> the </item>
				Swan <item repeat='0-1'><ruleref uri='#number'/></item> is
				<one-of>
					<item> DHARMA </item>
					<item> HATCH </item>
					<item> SWAN with no A </item>
					<item> SWAN no A </item>
					<item> DHARMA with no H </item>
					<item> DHARMA no H </item>
					<item> SWAN </item>
					<item> HITCH </item>
					<item> HATCH with no A </item>
					<item> HATCH no A </item>
					<item> 77 </item>
				</one-of>
			</item>
		</one-of>
	</rule>
	<rule id='selectModule'>
		<one-of>
			<item> module <ruleref uri='#number2'/></item>
			<item> first module </item>
			<item> next module </item>
			<item>
				<one-of>
					<item>
						<one-of>
							<item> is there </item>
							<item> do you have </item>
							<item> do we have </item>
						</one-of>
						<item repeat='0-1'><one-of><item> a </item><item> an </item></one-of></item>
					</item>
					<item>
						select
						<item repeat='0-1'><one-of><item> a </item><item> an </item><item> the </item></one-of></item>
					</item>
				</one-of>
				<ruleref uri='#module'/>
			</item>
			<item> vanilla modules </item>
		</one-of>
	</rule>
